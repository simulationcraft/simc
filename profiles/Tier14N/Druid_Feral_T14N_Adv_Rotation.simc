chest=eternal_blossom_raiment,id=85313,gems=160agi_160agi,enchant=80all
wrists=smooth_beetle_wristbands,id=86185,enchant=180agi,reforge=exp_hit
hands=murderers_gloves,id=85828,gems=80agi_160hit_60agi,enchant=170mastery
waist=cannoneers_gunpowder_carrier,id=90451,gems=160agi_160agi_60crit,reforge=crit_exp
legs=eternal_blossom_legguards,id=85310,gems=160agi_60agi,enchant=285agi_165crit,reforge=haste_mastery
