warrior="T23_Warrior_Protection"
source=default
spec=protection
level=120
race=tauren
role=tank
position=front
talents=1223211

# Default consumables
potion=battle_potion_of_strength
flask=undertow
food=bountiful_captains_feast
augmentation=battle_scarred

# This default action priority list is automatically created based on your character.
# It is a attempt to provide you with a action list that is both simple and practicable,
# while resulting in a meaningful and good simulation. It may not result in the absolutely highest possible dps.
# Feel free to edit, adapt and improve it to your own needs.
# SimulationCraft is always looking for updates and improvements to the default action lists.

# Executed before combat begins. Accepts non-harmful actions only.
actions.precombat=flask
actions.precombat+=/food
actions.precombat+=/augmentation
# Snapshot raid buffed stats before combat begins and pre-potting is done.
actions.precombat+=/snapshot_stats
actions.precombat+=/potion

# Executed every time the actor is available.
actions=auto_attack
actions+=/intercept
actions+=/use_item,name=ramping_amplitude_gigavolt_engine
actions+=/blood_fury
actions+=/berserking
actions+=/arcane_torrent
actions+=/lights_judgment
actions+=/fireblood
actions+=/ancestral_call
actions+=/call_action_list,name=prot

actions.prot=potion,if=target.time_to_die<25
actions.prot+=/avatar,if=(cooldown.demoralizing_shout.ready|cooldown.demoralizing_shout.remains>2)
actions.prot+=/demoralizing_shout
actions.prot+=/ravager
actions.prot+=/dragon_roar
actions.prot+=/thunder_clap,if=(talent.unstoppable_force.enabled&buff.avatar.up&debuff.demoralizing_shout_debuff.up)
actions.prot+=/shield_block,if=(cooldown.shield_slam.ready&buff.shield_block.down&buff.last_stand.down)
actions.prot+=/last_stand,if=buff.shield_block.down
actions.prot+=/shield_slam
actions.prot+=/thunder_clap
actions.prot+=/revenge,if=(!talent.vengeance.enabled)|(talent.vengeance.enabled&buff.revenge.react&!buff.vengeance_ignore_pain.up)|(buff.vengeance_revenge.up)|(talent.vengeance.enabled&!buff.vengeance_ignore_pain.up&!buff.vengeance_revenge.up&rage>=30)
actions.prot+=/ignore_pain,use_off_gcd=1,if=rage>70
actions.prot+=/devastate

