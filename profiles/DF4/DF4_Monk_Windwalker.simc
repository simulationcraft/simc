actions+=/spear_hand_strike,if=target.debuff.casting.react

# Potion
actions+=/potion
# Build Chi at the start of combat

# TP to not waste energy
actions+=/tiger_palm,if=chi<5&(energy>110&!talent.inner_peace|energy>140&!talent.inner_peace)&combo_strike
