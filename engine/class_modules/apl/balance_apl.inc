st->add_action( "incarnation,if=variable.cd_condition_st" );
st->add_action( "warrior_of_elune" );
st->add_action( "variable,name=enter_eclipse,value=eclipse.any_next|buff.eclipse_lunar.up&(buff.eclipse_lunar.remains<action.wrath.execute_time)|buff.eclipse_solar.up&(buff.eclipse_solar.remains<action.wrath.execute_time)" );
